							<input type="date" class="form-control" id="startDate" ng-if="vm.EnterpriseAppointment.AddOrModifyGroupEnterpriseInfoCategory.Id == undefined" ng-model="vm.EnterpriseAppointment.AddOrModifyGroupEnterpriseInfoCategory.ConvertMinAppointDate" placeholder="请输入开始时间"/>
							<input type="date" disabled class="form-control" id="startDate" ng-if="vm.EnterpriseAppointment.AddOrModifyGroupEnterpriseInfoCategory.Id != undefined" ng-model="vm.EnterpriseAppointment.AddOrModifyGroupEnterpriseInfoCategory.ConvertMinAppointDate" placeholder="请输入开始时间"/>
								                    <div class="input-group-addon">~</div>
