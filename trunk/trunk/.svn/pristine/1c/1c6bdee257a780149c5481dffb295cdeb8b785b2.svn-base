<section class="enterpriseAppointment">
	<div class="panel panel-default banner-paneller">
		<!-- heading -->
		<div class="panel-heading" >
			<ul class="breadcrumb">
				<li><a href="#">掌上体检</a></li>
				<li class="active">团检企业管理</li>
			</ul>
		</div>
		<!-- body -->
		<div class="panel-body answers_pane">
		    <!-- search -->
			<div class="clearfix">
			    <div class="form-inline">
			    	<form class="pull-left form-inline">
			    		<div class="form-group">
			    			<span>企业团检名称：</span>
			    			<input type="text" class="form-control input-sm" ng-model="vm.EnterpriseAppointment.Search.Name" placeholder="请输入" />
			    		</div>
			    		<!-- <div class="form-group">
			    			<span>体检中心：</span>
			    			<input type="text" class="form-control input-sm" ng-model="vm.EnterpriseAppointment.Search." placeholder="请输入" />
			    		</div> -->
			    		<div class="form-group">
			    			<span>体检时间段：</span>
			    			<div class="input-group input-group-sm">  
			                   <input type="text" id="EnterpriseAppointmentStartTime" class="form-control" placeholder="请选择起始时间" 
			                          style="width:110px; border-right:0;" />
			                   <span class="input-group-addon">-</span>
			                   <input type="text" id="EnterpriseAppointmentEndTime" class="form-control" placeholder="请选择结束时间" 
			                          style="width:110px; border-left: 0;" />
			                </div>
			    		</div>
			    	</form>
			    	<div class="pull-right">
						<div class="form-group" style="margin-top: 5px;">
			    	    	<label ng-bind="vm.EnterpriseAppointment.pageModel.totalItems"></label>条
			    	    </div>
			    		<div class="form-group">
			    			<button class="btn btn-sm btn-info" ng-click="vm.EnterpriseAppointment.OnSearchEnterpriseClick()">
			    				<span class="glyphicon"></span>
			    				搜索
			    			</button>
			    		</div>
			    		<div class="form-group">
			    			<button class="btn btn-sm btn-info" ng-click="vm.EnterpriseAppointment.ShowAddEnterpriseModelDialog()">
			    				<span class="glyphicon"></span>
			    				添加团检企业
			    			</button>
			    		</div>
			    	</div>
			    </div>
			</div>
			<!-- table -->
			<br />
			<table class="table table-bordered">
				<thead>
					<tr>
						<th>序号</th>
						<th>企业订单号</th>
						<th>企业名称</th>
						<th>体检中心</th>
						<th>体检时间段</th>
						<th>联系人姓名</th>
						<th>联系人电话</th>
						<th>单位地址</th>
						<th>员工列表</th>
						<th>报告寄送方式</th>
						<th>是否出具总检报告</th>
						<th>添加时间</th>
						<th>操作</th>
					</tr>
				</thead>
				<tbody>
					<tr ng-repeat="item in vm.EnterpriseAppointment.GroupAppointmentEnterpriseList track by $index">
						<td>{{$index + 1}}</td>
						<td ng-bind="item.Id"></td>
						<td ng-bind="item.Name"></td>
						<td ng-bind="item.AppointmentAgency"></td>
						<td ng-bind="item.AppointmentTimeRange"></td>
						<td ng-bind="item.ConnectName"></td>
						<td ng-bind="item.ConnectMobile"></td>
						<td ng-bind="item.Address"></td>
						<td>
		                    <button type="button" class="btn btn-success btn-xs"
		                    		ng-click="vm.EnterpriseAppointment.OnManagementStaff(item)">
		                      	 管理
		                    </button>
						</td>
						<td ng-bind="item.ConvertReportSendType"></td>
						<td ng-bind="item.ConvertReportSummary"></td>
						<td ng-bind="item.CreateTime"></td>
						<td>
							<button class="btn btn-sm btn-success" ng-click="vm.EnterpriseAppointment.ShowInspectEnterpriseModelDialog(item)">
								查看
							</button>
							<button class="btn btn-sm btn-info" ng-click="vm.EnterpriseAppointment.ShowModifyEnterpriseModelDialog(item)">
								编辑
							</button>
							<button class="btn btn-sm btn-warning" ng-click="vm.EnterpriseAppointment.ShowDeleteEnterpriseModelDialog(item)">
								删除
							</button>
						</td>
					</tr>
				</tbody>
			</table>
			<br />
			<tm-pagination class="pull-right" conf="EnterprisePaginationConf"></tm-pagination>
		</div>
	</div>
</section>

<!-- StartaddOrModifyEnterpriseAppointmentCompanyModal -->
