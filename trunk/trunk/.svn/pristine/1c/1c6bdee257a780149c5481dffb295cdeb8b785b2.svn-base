								<td style="width:23%;text-align: left;"><span>{{item.Id}}</span></td>
								<td style="width:23%;text-align: left;"><input type="text" ng-model="item.GroupAppointmentPrice" placeholder="请输入价格" /></td>
								<td style="width:4%;text-align: right;"><button class="btn btn-sm btn-warning" ng-click="vm.EnterpriseAppointment.RemoveFirstAddingGoodsInfo(item)">
									<span class="glyphicon glyphicon-minus-sign"></span>
								</button></td>
							</tr>
						</tbody>
					</table>
					<!-- 编辑 -->
					<table class="table" ng-if="vm.EnterpriseAppointment.AddOrModifyGroupEnterpriseInfoCategory.Id != undefined">
						<tbody>
							<tr ng-repeat="item in vm.EnterpriseAppointment.CurrentEnterpriseGoodsInfo">
								<td style="width:23%;text-align: left;"><span>{{item.SupplierName}}</span></td>
								<td style="width:23%;text-align: left;"><span>{{item.GoodsName}}</span></td>
								<td style="width:23%;text-align: left;"><span>{{item.GoodsId}}</span></td>
								<td style="width:23%;text-align: left;"><span>{{item.GroupSalesPrise}}</span></td>
