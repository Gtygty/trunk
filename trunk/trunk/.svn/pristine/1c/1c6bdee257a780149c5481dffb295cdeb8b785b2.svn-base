<!-- addServiceProviderModal -->
<div class="modal fade" id="addServiceProviderModal">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<div class="modal-header">
				<button class="close" data-dismiss="modal">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title">选择</h4>
			</div>
			<div class="modal-body" style="padding: 20px 18px;">
				<div class="form-group">
					<span>体检服务商：</span>
					<input class="input-sm form-control" style="display: inline-block;width:65%;" type="text" placeholder="请输入体检服务商名称" ng-model="vm.EnterpriseAppointment.SearchGoodsSupplier" />
					<div class="pull-right" style="width:24%;">
			            <button type="button" class="btn btn-success btn-sm" ng-click="vm.EnterpriseAppointment.OnSearchGoodsSupplier()">
			              <span class="glyphicon glyphicon-search"></span>&nbsp;搜索
			            </button>
			            <button type="button" class="btn btn-info btn-sm" ng-click="vm.EnterpriseAppointment.OnClearGoodsSupplier()">
			              <span class="glyphicon glyphicon-trash"></span>&nbsp;清除
			            </button>
		            </div>
				</div>
				<div class="form-group enterpriseAppointment_boxOne">
					<div class="enterpriseAppointment_divOne">
					    <label ng-repeat="item in vm.EnterpriseAppointment.GroupEnterpriseList" 
					    ng-class="{true:'enterpriseAppointment_labelTwo',false:'enterpriseAppointment_labelOne'}[item.isChecked]"
					    ng-click="vm.EnterpriseAppointment.OnGetGoodsInfo(item, $index)">

				    <!-- <div class="enterpriseAppointment_divOne">
					    <label ng-repeat="item in vm.EnterpriseAppointment.GroupEnterpriseList" 
					    ng-class="{true:'enterpriseAppointment_labelTwo',false:'enterpriseAppointment_labelOne'}[item.isChecked]"
					    ng-click="vm.EnterpriseAppointment.OnGetGoodsInfo(item)"> -->
					        {{item.SupplierName}}
					    </label>
					</div>
					<div class="enterpriseAppointment_divOne">
					    <label ng-repeat="item in vm.EnterpriseAppointment.CurrentGoodsList" 
					    ng-class="{true:'enterpriseAppointment_labelTwo',false:'enterpriseAppointment_labelOne'}[item.isChecked]"
					    ng-click="vm.EnterpriseAppointment.OnGetAllGoodsInfo($index,item)"> {{item.CommonInfo.Name}}
					    </label>
					</div>
					<div class="enterpriseAppointment_divTwo">
						<div  ng-repeat="item in vm.EnterpriseAppointment.AllGoodsList">
						    <label  class="enterpriseAppointment_labelTwo">
						    	{{item.SupplierName}}
						        <button class="close" style="font-size: 20px;background-color:#ff6600;border-radius: 50%;">
									<span ng-click="vm.EnterpriseAppointment.OnRemoveGoodsInfo($index,item,1)" style="color:#fff;">×</span>
								</button>
						    </label>
					        <label ng-repeat="cItem in item.GoodsInfoList" 
					               class="enterpriseAppointment_labelOne">
						    	{{cItem.CommonInfo.Name}}
						        <button class="close" style="font-size: 20px;background-color:#ff6600;border-radius: 50%;">
									<span ng-click="vm.EnterpriseAppointment.OnRemoveGoodsInfo($index,cItem,2)" style="color:#fff;">×</span>
								</button>
						    </label>
						</div>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button class="btn btn-sm btn-warning" data-dismiss="modal">
					取消
				</button>
				<button class="btn btn-sm btn-info" ng-click="vm.EnterpriseAppointment.AddAppointmentServiceProvider()">
					<span class="glyphicon"></span>
					保存
				</button>
			</div>
		</div>
	</div>
</div>
