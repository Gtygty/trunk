/*--------------table--------------*/
.BMS_table .table>tbody>tr>th {
  text-align: center;
}
.BMS_table_border .table>tbody>tr>th,
.BMS_table_border .table>tbody>tr>td {
  border: 0;
}
/*--------------模态框--------------*/
.clonzing {
  width: 300px;
  height: auto;
  margin: 25% auto;
  border-radius: 6px;
}
.clonzing .modal-body {
  padding: 0;
}
.clonzing button{
  margin-bottom: 10px;
}
.clonzing h3 {
  padding-top: 40px;
  padding-bottom: 40px;
  margin-bottom: 10px;
  font-family: 'Arial Normal', 'Arial';
  font-size: 20px;
  font-weight: 400;
  border-bottom: 1px solid #ddd;
}
.Adding {
  width: 500px;
  margin: 100px auto;
  border-radius: 6px;
}
.Adding .modal-body{
  padding: 15px;
}
.Adding .modal-footer{
  text-align: center;
}